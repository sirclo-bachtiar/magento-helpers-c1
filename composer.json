{
    "name": "sirclo-bachtiar/magento-helpers-v2",
    "description": "Magento Helpers v2",
    "type": "library",
    "version": "1.1.4",
    "license": "MIT",
    "keywords": [
        "logger",
        "laminas"
    ],
    "support": {
        "issues": "https://github.com/sirclo-bachtiar/magento-helpers-v2/issues",
        "source": "https://github.com/sirclo-bachtiar/magento-helpers-v2"
    },
    "authors": [
        {
            "name": "sirclo-bachtiar",
            "email": "tio.bachtiar@sirclo.com"
        }
    ],
    "require": {
        "laminas/laminas-log": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Bachtiar\\Helper\\LaminasLogger\\": "src/laminas-logger",
            "Bachtiar\\Helper\\CustomQueryGenerator\\": "src/custom-query-generator",
            "Bachtiar\\Helper\\ConsoleService\\": "src/console-service"
        }
    },
    "homepage": "https://github.com/sirclo-bachtiar/magento-helpers-v2",
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
